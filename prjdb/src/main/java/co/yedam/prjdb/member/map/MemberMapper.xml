<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.yedam.prjdb.member.map.MemberMapper">
	<select id="memberSelectList" resultType="co.yedam.prjdb.member.service.MemberVO">
	SELECT * FROM MEMBER
	</select>
	
	<select id="memberSelect" resultType="co.yedam.prjdb.member.service.MemberVO">
	SELECT * FROM MEMBER WHERE MEMBER_ID=#{memberId}
	<if test="memberPassword != null">
		AND MEMBER_PASSWORD = #{memberPassword}
	</if>
	</select>
	
	<insert id="memberInsert" parameterType="co.yedam.prjdb.member.service.MemberVO">
		INSERT INTO MEMBER(member_id, member_password, member_name, member_tel)
		VALUES(#{memberId}, #{memberPassword}, #{memberName}, #{memberTel})
	</insert>
	
	<update id="memberUpdate" parameterType="co.yedam.prjdb.member.service.MemberVO"></update>
	
	<delete id="memberDelete" parameterType="co.yedam.prjdb.member.service.MemberVO"></delete>
</mapper>